<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/GreenDaoMeasureButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/GreenDao"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/RoomMeasureButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Room"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/GreenDaoMeasureButton" />

    <Button
        android:id="@+id/DBFlowMeasureButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/DBFlow"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/RoomMeasureButton" />

    <Button
        android:id="@+id/ORMLiteMeasureButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ORMLite"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/DBFlowMeasureButton" />

    <Button
        android:id="@+id/JDBCMeasureButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/JDBC"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ORMLiteMeasureButton" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/JDBCMeasureButton"
        android:text="ANYTHING!?"/>

</androidx.constraintlayout.widget.ConstraintLayout>